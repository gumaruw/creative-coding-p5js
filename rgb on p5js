function setup() {
	simple();
}

function draw() {
	repeat(128,balloon);
	
}

function balloon(){
	noStroke();
	var x=randomNumber(800);
	var y=randomNumber(600);
	var size=randomNumber(10,40);
	
	var r=randomNumber(255);
	//var g=randomNumber(255);
	//var b=randomNumber(255);
	//fill(r, g, b);
	fill(r,x/6,x/2);
	//played a lil game here :).
	circle(x,y,size);
}
